            android:name=".LoginActivity"
            android:label="@string/text_title_login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MapActivity"
            android:label="@string/text_title_map" />
        <activity
            android:name=".RecordActivity"
            android:label="@string/text_title_record" />
        <activity
            android:name=".RegisterActivity"
            android:label="@string/text_title_register" />
        <activity
            android:name=".ResultsActivity"
            android:label="@string/text_title_feeling" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/text_title_settings" />
