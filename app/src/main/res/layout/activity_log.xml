        android:textSize="@dimen/title_text_size"
        android:textStyle="bold" />

    <Button
        android:id="@+id/btn_sad2"
        android:layout_width="74dp"
        android:layout_height="68dp"
        android:layout_above="@+id/textView16"
        android:layout_alignStart="@+id/btn_happy4"
        android:layout_marginBottom="1dp"
        android:background="@drawable/ic_sad"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/btn_happy3"
        android:layout_width="73dp"
        android:layout_height="66dp"
        android:layout_alignBaseline="@+id/textView6"

        android:layout_alignStart="@+id/btn_happy2"
        android:background="@drawable/ic_happy_green"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/btn_happy2"
        android:layout_width="76dp"
        android:layout_height="67dp"
        android:layout_alignParentLeft="true"

        android:layout_alignParentStart="true"

        android:layout_alignParentTop="true"
        android:layout_marginLeft="28dp"

        android:layout_marginStart="28dp"

        android:layout_marginTop="58dp"
        android:background="@drawable/ic_happy_green"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btn_happy2"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="33dp"
        android:layout_marginRight="33dp"
        android:text="Good Presentation"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView11"
        android:layout_alignTop="@+id/textView8"
        android:text="Yesterday" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView2"
        android:layout_below="@+id/btn_happy2"
        android:text="13:00" />

</RelativeLayout>